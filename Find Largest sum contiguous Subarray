//Brute Force Approach
#include<bits/stdc++.h>
using namespace std;

int main()
{
	int n;
	cin>>n;
	int arr[n+1];
	
	for(int i=1;i<=n;i++)
	{
		cin>>arr[i];
	}	
	
	int max=INT_MIN;
	for(int i=1;i<=n;i++)
	{
		for(int j=i;j<=n;j++)
		{
			int s=0;
			for(int k=i;k<=j;k++)
			{
				s=s+arr[k];
			}	
			if(s>max)
			{
                max=s;
			}	
		}	
	}	
	cout<<max;
}
